{
  "name": "MICCAI 2024 Paper Listings",
  "fields": [
    {
      "name": "title",
      "type": "text",
      "selector": ".posts-list-item-name b"
    },
    {
      "name": "authors",
      "type": "list",
      "fields": [
        {
          "name": "author_name",
          "type": "text"
        }
      ],
      "selector": "ul li a[href^=\"/miccai-2024/tags\"]"
    },
    {
      "name": "paper_url",
      "type": "attribute",
      "selector": "li a[href*=\"Paper\"]",
      "attribute": "href"
    },
    {
      "name": "pdf_url",
      "type": "attribute",
      "selector": "a[href*=\"paper.pdf\"]",
      "attribute": "href"
    }
  ],
  "baseSelector": ".posts-list-item"
}